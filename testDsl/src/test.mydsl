// datatypes.dmodel
 
datatype String

// commons.dmodel
 
package my.company.common {
	
    entity HasAuthor {
        author: String
    }
}

// blogs.dmodel
     
package my.company.blog {
  
    import my.company.common.*

    entity Blog {
        title: String
        many posts: Post
    }

    entity Post extends my.company.common.HasAuthor {
        title: String
        content: String
        many comments: Comment
    }

    entity Comment extends HasAuthor {
        content: String
    }
}